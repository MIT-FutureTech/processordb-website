{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build && npm run copy-client-assets",
    "copy-client-assets": "node scripts/copy-client-assets.js",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "npx eslint --ext .vue --ignore-pattern .gitignore .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:unit": "vitest run tests/components tests/lib",
    "test:integration": "vitest run tests/api",
    "test:frontend": "playwright test",
    "test:frontend:ui": "playwright test --ui",
    "test:frontend:debug": "playwright test --debug",
    "test:frontend:headed": "playwright test --headed",
    "test:accessibility": "playwright test --grep @accessibility",
    "test:performance": "playwright test --grep @performance",
    "test:visual": "playwright test --grep @visual",
    "test:forms": "playwright test --grep @forms",
    "test:all": "npm run test:run && npm run test:frontend",
    "test:service": "node scripts/test-service.js",
    "test:coverage": "vitest run --coverage",
    "test:validate": "node scripts/validate-test-config.js",
    "health": "curl -f http://localhost:3000/health || exit 1"
  },
  "dependencies": {
    "@heroicons/vue": "^2.2.0",
    "@primevue/themes": "^4.2.3",
    "@tanstack/vue-query": "^5.66.0",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "highcharts": "^11.4.8",
    "highcharts-vue": "^2.0.1",
    "jspdf": "^3.0.1",
    "jszip": "^3.10.1",
    "lucide-vue-next": "^0.453.0",
    "nuxt": "^3.20.1",
    "nuxt-plotly": "1.0.12",
    "oh-vue-icons": "^1.0.0-rc3",
    "postcss": "^8.4.49",
    "primevue": "^4.2.3",
    "radix-vue": "^1.9.7",
    "shadcn-nuxt": "^0.10.4",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.4.17",
    "vue": "^3.5.0",
    "vue-router": "^4.3.3",
    "vue-sidebar-menu": "^5.4.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.23.0",
    "@nuxt/eslint-config": "^1.2.0",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@playwright/test": "^1.40.0",
    "@primevue/nuxt-module": "^4.2.3",
    "@types/node": "^24.5.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^9.24.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "jsdom": "^23.0.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.0",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.1.3"
  },
   "overrides": {
    "inflight": "npm:@isaacs/inflight@^1.0.6",
    "glob": "^10.5.0",
    "rimraf": "^5.0.0",
    "lodash.isequal": "npm:lodash@^4.17.21"
  }
}
